@Article{Lu2021,
  author   = {Lu, Yang},
  journal  = {Scandinavian Journal of Statistics},
  title    = {The predictive distributions of thinning-based count processes},
  year     = {2021},
  number   = {1},
  pages    = {42-67},
  volume   = {48},
  abstract = {Abstract This paper shows that the term structure of conditional (i.e. predictive) distributions allows for closed form expression in a large family of (possibly higher order or infinite order) thinning-based count processes such as INAR(p), INARCH(p), NBAR(p), and INGARCH(1,1). Such predictive distributions are currently often deemed intractable by the literature and existing approximation methods are usually time consuming and induce approximation errors. In this paper, we propose a Taylor's expansion algorithm for these predictive distributions, which is both exact and fast. Through extensive simulation exercises, we demonstrate its advantages with respect to existing methods in terms of the computational gain and/or precision.},
  doi      = {https://doi.org/10.1111/sjos.12438},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/sjos.12438},
  keywords = {compound autoregressive process, matrix arithmetic, probabilistic forecast of counts},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1111/sjos.12438},
}

@Article{Kirchner2016,
  author   = {M. Kirchner},
  journal  = {Stochastic Processes and their Applications},
  title    = {Hawkes and {INAR}(inf) processes},
  year     = {2016},
  issn     = {0304-4149},
  number   = {8},
  pages    = {2494-2525},
  volume   = {126},
  abstract = {In this paper, we discuss integer-valued autoregressive time series (INAR), Hawkes point processes, and their interrelationship. Besides presenting structural analogies, we derive a convergence theorem. More specifically, we generalize the well-known INAR(p), p∈N, time series model to a corresponding model of infinite order: the INAR(∞) model. We establish existence, uniqueness, finiteness of moments, and give formulas for the autocovariance function as well as for the joint moment-generating function. Furthermore, we derive a branching-process–as well as an AR(∞)–and an MA(∞) representation for the model. We compare Hawkes process properties with their INAR(∞) counterparts. Given a Hawkes process N, in the main theorem of the paper we construct an INAR(∞)-based family of point processes and prove its convergence to N. This connection between INAR and Hawkes models will be relevant in applications.},
  doi      = {https://doi.org/10.1016/j.spa.2016.02.008},
  keywords = {Hawkes process, Integer-valued time series, Weak convergence of point processes, Branching process},
  url      = {https://www.sciencedirect.com/science/article/pii/S0304414916000399},
}

